  alert("You have new messages to read!í²¡");
}

function fun() {
var a = confirm("Do you want to proceed with the blood donation process?");
    if (a)
    {
        window.location = "https://www.jotform.com/build/220212172223437";
    }
    else
   {
        window.location = "";
    }
}
</script>
